{
  "name": "how-green",
  "version": "0.1.0",
  "keywords": [],
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "postinstall": "yarn build",
    "test": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "axios": "^0.21.1",
    "bootstrap": "4.5.3",
    "bootstrap-vue": "^2.21.2",
    "chart.js": "2",
    "vue": "2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.2.1",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-vue": "^8.1.1",
    "lint-staged": "^12.1.2",
    "node-sass": "^6.0.1",
    "sass-loader": "^10.2.0",
    "sass-resources-loader": "^2.2.3",
    "vue-template-compiler": "^2.6.14"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "black app && lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
